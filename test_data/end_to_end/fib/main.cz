fn fib(_ v: i32) -> i32 {
    if v <= 1 {
      v
    } else {
        let a = v - 1;
        let b = v - 2;
        fib(a) + fib(b)
    }
}

fn print_fib(_ v: i32) {
    prints("fib("); print_i32(v); prints("): "); print_i32(fib(v)); println();
}

fn main() {
    print_fib(0 - 100);
    print_fib(0);
    print_fib(1);
    print_fib(10);
    print_fib(20);
}
