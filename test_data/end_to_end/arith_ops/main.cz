fn main() {
    prints("==================== i8\n");
    do_i8(0, 0);
    do_i8(0, 1);
    do_i8(126, 127);
    do_i8(-126, -127);
    do_i8(42, 2);
    do_i8(42, -3);

    prints("==================== u8\n");
    do_u8(0, 0);
    do_u8(0, 1);
    do_u8(254, 255);
    do_u8(126, 2);
    do_u8(254, 2);

    prints("==================== i16\n");
    do_i16(0, 0);
    do_i16(0, 1);
    do_i16(32766, 32767);
    do_i16(-32768, -32767);
    do_i16(420, 2);
    do_i16(420, -3);

    prints("==================== u16\n");
    do_u16(0, 0);
    do_u16(0, 1);
    do_u16(65534, 65535);
    do_u16(32766, 2);
    do_u16(65534, 2);

    prints("==================== i32\n");
    do_i32(0, 0);
    do_i32(0, 1);
    do_i32(0x7fff_fffe, 0x7fff_ffff);
    do_i32(-0x8000_0000, -0x7fff_ffff);
    do_i32(420000, 2);
    do_i32(420000, -3);

    prints("==================== u32\n");
    do_u32(0, 0);
    do_u32(0, 1);
    do_u32(0xffff_fffe, 0xffff_ffff);
    do_u32(0x7fff_fffe, 2);
    do_u32(0xffff_fffe, 2);

    prints("==================== i64\n");
    do_i64(0, 0);
    do_i64(0, 1);
    do_i64(0x7fff_ffff_ffff_fffe, 0x7fff_ffff_ffff_ffff);
    do_i64(-0x8000_0000_0000_0000, -0x7fff_ffff_ffff_ffff);
    do_i64(420_000_000_000, 2);
    do_i64(420_000_000_000, -3);

    prints("==================== u64\n");
    do_u64(0, 0);
    do_u64(0, 1);
    do_u64(0xffff_ffff_ffff_fffe, 0xffff_ffff_ffff_ffff);
    do_u64(0x7fff_ffff_ffff_fffe, 2);
    do_u64(0xffff_ffff_ffff_fffe, 2);

    prints("==================== i128\n");
    do_i128(0, 0);
    do_i128(0, 1);
    do_i128(0x7fff_ffff_ffff_ffff_ffff_ffff_ffff_fffe, 0x7fff_ffff_ffff_ffff_ffff_ffff_ffff_ffff);
    do_i128(-0x8000_0000_0000_0000_0000_0000_0000_0000, -0x7fff_ffff_ffff_ffff_ffff_ffff_ffff_ffff);
    do_i128(420_000_000_000_000_000_000_000_000_000, 2);
    do_i128(420_000_000_000_000_000_000_000_000_000, -3);

    prints("==================== u128\n");
    do_u128(0, 0);
    do_u128(0, 1);
    do_u128(0xffff_ffff_ffff_ffff_ffff_ffff_ffff_fffe, 0xffff_ffff_ffff_ffff_ffff_ffff_ffff_ffff);
    do_u128(0x7fff_ffff_ffff_ffff_ffff_ffff_ffff_fffe, 2);
    do_u128(0xffff_ffff_ffff_ffff_ffff_ffff_ffff_fffe, 2);

    prints("==================== isize\n");
    do_isize(0, 0);
    do_isize(0, 1);
    do_isize(42, 43);
    do_isize(-43, -42);

    prints("==================== usize\n");
    do_usize(0, 0);
    do_usize(1, 0);
    do_usize(43, 42);

    prints("==================== f32\n");
    do_f32(0.0, 0.0);
    do_f32(1.234, 0.234);
    do_f32(-42.345, 42.345);

    prints("==================== f64\n");
    do_f64(0.0, 0.0);
    do_f64(1.234, 0.234);
    do_f64(-42.345, 42.345);
}
