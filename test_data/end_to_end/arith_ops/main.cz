fn do_i8(_ a: i8, _ b: i8) {
    print_i8(a); prints(" + "); print_i8(b); prints(" = "); print_i8(a + b); println();
    print_i8(a); prints(" - "); print_i8(b); prints(" = "); print_i8(a - b); println();
    prints("-"); print_i8(a); prints(" = "); print_i8(-a); println();
    prints("-"); print_i8(b); prints(" = "); print_i8(-b); println();
    print_i8(a); prints(" * "); print_i8(b); prints(" = "); print_i8(a * b); println();
    if b != 0 {
        print_i8(a); prints(" / "); print_i8(b); prints(" = "); print_i8(a / b); println();
    }
    if b != 0 {
        print_i8(a); prints(" % "); print_i8(b); prints(" = "); print_i8(a % b); println();
    }
    println();
}

