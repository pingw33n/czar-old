fn do_i8(_ a: i8, _ b: i8) {
    print_i8(a + b);
    print_i8(a - b);
    print_i8(-a);
    print_i8(-b);
    print_i8(a * b);
    if b > 0 {
        print_i8(a / b);
    }
    if b > 0 {
        print_i8(a % b);
    }
    println();
}

