fn cmp(_ a: i32, _ b: i32) {
    print_bool(a == b);
    print_bool(a != b);
    print_bool(a < b);
    print_bool(a <= b);
    print_bool(a > b);
    print_bool(a >= b);
    println();
}

fn main() {
    cmp(0, 0);
    cmp(-2_147_483_648, -2_147_483_648);
    cmp(2_147_483_647, 2_147_483_647);
    cmp(-2_147_483_648, -2_147_483_647);
    cmp(2_147_483_646, 2_147_483_647);
}
